<Window x:Class="FPSResultsAnalyzer.Views.SaveCSVDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FPSResultsAnalyzer.Views"
        mc:Ignorable="d"
        Title="CSV file save" Width="400" Height="100">
    <Grid>
        <StackPanel>
            <TextBlock Text="CSV file path:" FontWeight="Bold" />
            <TextBox x:Name="CSVPathBox" />
            <Button Content="OK" Click="OKButton_Click" />
        </StackPanel>
    </Grid> 
</Window>
